{
  "uid": "pg-0b201a72",
  "title": "PostgreSQL RDS Overview",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "timezone": "browser",
  "tags": [
    "postgres",
    "rds",
    "prometheus"
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROM",
        "query": "prometheus",
        "label": "Datasource",
        "refresh": 1
      },
      {
        "type": "query",
        "name": "job",
        "label": "Job",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values(up{job!=\"\"}, job)",
        "refresh": 1,
        "current": {
          "selected": true,
          "text": "postgres",
          "value": "postgres"
        }
      },
      {
        "type": "query",
        "name": "instance",
        "label": "Instance",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values(up{job=\"$job\"}, instance)",
        "refresh": 1,
        "includeAll": true,
        "multi": true
      },
      {
        "type": "query",
        "name": "datname",
        "label": "Database",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values(pg_stat_database_xact_commit{job=\"$job\",instance=~\"$instance\"}, datname)",
        "refresh": 1,
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Exporter up",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "expr": "avg without(instance) (up{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "up",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Connections (total)",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "expr": "sum by(instance) (pg_stat_activity_count{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Transactions per second (TPS)",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "expr": "sum by(instance) (rate(pg_stat_database_xact_commit{job=\"$job\",instance=~\"$instance\",datname=~\"$datname\"}[5m]) + rate(pg_stat_database_xact_rollback{job=\"$job\",instance=~\"$instance\",datname=~\"$datname\"}[5m]))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Rollbacks per second",
      "gridPos": {
        "x": 12,
        "y": 6,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "targets": [
        {
          "expr": "sum by(instance) (rate(pg_stat_database_xact_rollback{job=\"$job\",instance=~\"$instance\",datname=~\"$datname\"}[5m]))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ]
    }
  ]
}